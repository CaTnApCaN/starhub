local Window = Fluent:CreateWindow({
    Title="Star Hub",
    SubTitle="T峄昻g H峄 Script",
    TabWidth=157,
    Size=UDim2.fromOffset(450,300),
    Acrylic=true,
    Theme="Dark",
    MinimizeKey=Enum.KeyCode.End
})

